{
    "name": "Ninja Printer",
    "version": "1.1.0",
    "manifest_version": 3,
    "description": "Westwing printing solution",
    "background": {
        "service_worker": "service_worker.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.west-wing.ws/*",
                "*://*.westwing.es/*",
                "*://*.westwing.fr/*",
                "*://*.westwing.pl/*",
                "*://*.westwing.de/*",
                "*://*.westwing.eu/*",
                "*://*.westwingnow.es/*",
                "*://*.westwingnow.fr/*",
                "*://*.westwingnow.pl/*",
                "*://*.westwingnow.de/*",
                "*://*.westwingnow.eu/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "NinjaPrinter.js"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "icons": {
        "128": "ninja-128.png"
    },
    "permissions": [
        "nativeMessaging"
    ],
    "host_permissions": [
        "*://*.west-wing.ws/*",
        "*://*.westwing.es/*",
        "*://*.westwing.fr/*",
        "*://*.westwing.pl/*",
        "*://*.westwing.de/*",
        "*://*.westwing.eu/*",
        "*://*.westwingnow.es/*",
        "*://*.westwingnow.fr/*",
        "*://*.westwingnow.pl/*",
        "*://*.westwingnow.de/*",
        "*://*.westwingnow.eu/*"
    ]
}
